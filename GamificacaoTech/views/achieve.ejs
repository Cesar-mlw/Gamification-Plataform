        data: <%-achiDev%>
    })
    var tabelaGames = prepareDataTable("#tabelaGames", {
        order: [[0, "asc"]],
        deferRender: true,
        columns: [
            { title: "ID", "class": "col-min", data: "id_achievement" },
            { title: "Nome", "class": "col-25", data: "nome_achievement" },
            { title: "Criterio Achievement", "class": "col-75", data: "criterio_achievement" },

        ],
        data: <%-achiGames%>
    })
    var tabelaInov = prepareDataTable("#tabelaInov", {
        order: [[0, "asc"]],
        deferRender: true,
        columns: [
            { title: "ID", "class": "col-min", data: "id_achievement" },
            { title: "Nome", "class": "col-25", data: "nome_achievement" },
            { title: "Criterio Achievement", "class": "col-75", data: "criterio_achievement" },

        ],
        data: <%-achiInov%>
    })

     var tabelaNComp = prepareDataTable("#tabelaNComp", {
        order: [[0, "asc"]],
        deferRender: true,
        columns: [
            { title: "ID", "class": "col-min", data: "id_achievement" },
            { title: "Nome", "class": "col-25", data: "nome_achievement" },
            { title: "Criterio Achievement", "class": "col-75", data: "criterio_achievement" },

        ],
        data: <%-achiNComp %>
    })
    function homeClick() {
        window.location.replace("/")
    }
</script>
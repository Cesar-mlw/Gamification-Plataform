<div class="achieveContainer">
    <button class="iconBtn botaoVoltar" onclick="homeClick()"><i class="fa fa-home"></i></button>
    <h1>BI</h1>
    <div class="tabelaTop">
        <table class="table table-striped table-hover" id="tabelaBI"></table>
    </div>
    <h1>Dev</h1>
    <div class="tabelaTop">
        <table class="table table-striped table-hover" id="tabelaDev"></table>
    </div>
    <h1>Games</h1>
    <div class="tabelaTop">
        <table class="table table-striped table-hover" id="tabelaGames"></table>
    </div>
    <h1>Inovação</h1>
    <div class="tabelaTop">
        <table class="table table-striped table-hover" id="tabelaInov"></table>
    </div>
    <h1>Achievements não completos</h1>
    <div class="tabelaTop">
        <table class="table table-striped table-hover" id="tabelaNComp"></table>
    </div>
</div>











<%- contentFor("scripts") %>
<script type="text/javascript">

   
    var tabelaBI = prepareDataTable("#tabelaBI", {
        order: [[0, "asc"]],
        deferRender: true,
        columns: [
            { title: "ID", "class": "col-min", data: "id_achievement" },
            { title: "Nome", "class": "col-25", data: "nome_achievement" },
            { title: "Criterio Achievement", "class": "col-75", data: "criterio_achievement" },

        ],
        data: <%-achiBI%>
    })
    var tabelaDev = prepareDataTable("#tabelaDev", {
        order: [[0, "asc"]],
        deferRender: true,
        columns: [
            { title: "ID", "class": "col-min", data: "id_achievement" },
            { title: "Nome", "class": "col-25", data: "nome_achievement" },
            { title: "Criterio Achievement", "class": "col-75", data: "criterio_achievement" },

